# Supabase Configuration
# Copy this file to .env.local and replace with your actual values

VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Optional: Debug mode
VITE_DEBUG=true
VITE_DEBUG_SUPABASE=true

# Optional: Feature flags
VITE_ENABLE_ANALYTICS=false
VITE_DEBUG_MODE=true
VITE_EXPERIMENTAL_FEATURES=false
VITE_OFFLINE_MODE=false

# Optional: Security settings
VITE_ENABLE_CSRF=true
VITE_SESSION_TIMEOUT=3600000
VITE_MAX_LOGIN_ATTEMPTS=5

# Optional: Performance settings
VITE_ENABLE_SW=false
VITE_ENABLE_CACHING=true
VITE_CACHE_TIMEOUT=300000

# PrintNode Configuration
VITE_PRINTNODE_API_KEY=your-printnode-api-key-here
VITE_PRINTNODE_PRINTER_ID=your-printer-id-here
